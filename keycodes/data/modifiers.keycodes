LeftControl  -> 0x0100 -> lc, lctrl, lctl
RightControl -> 0x1100 -> rc, rctrl, rctl
Control      -> 0x1100 -> c, ctrl, ctl
LeftShift    -> 0x0200 -> ls, lshift, lsft
RightShift   -> 0x1200 -> rs, rshift, rsft
Shift        -> 0x1200 -> s, sft
LeftAlt      -> 0x0400 -> la, lalt
RightAlt     -> 0x1400 -> ra, ralt
Alt          -> 0x1400 -> a
LeftGui      -> 0x0800 -> lg, lgui
RightGui     -> 0x1800 -> rg, rgui
Gui          -> 0x1800 -> g
